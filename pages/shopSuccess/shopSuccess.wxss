/* 购买成功页面 */
.shopSuccess{
  width: 100%;
  height: 100%;
  background: #EEF2F7 ;
  font-family: PingFangSC-Medium;
}
.shopSuccessTo{
  background: #fff;
  margin-bottom: 20rpx;
  text-align: center;
  padding-bottom: 104rpx;
}
.shopSuccessTo>view:nth-child(1){
  font-size: 40rpx;
  margin: 64rpx 0 94rpx;
  color: #5D9BE5;
  font-weight: bold;
}
.shopSuccessTo1{
  font-size: 26rpx;
}
.shopSuccessTo1>text{
  color: #4A90E2;
}
.shopSuccessTo2{
  background: #fff;
  padding: 48rpx 0 70rpx;
  text-align: center;
}
.shopSuccessTo2>view:nth-child(1){
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 50rpx;
}
.shopSuccessTo2>image:nth-child(2){
  width:100% ;
  height: 650rpx;
}
.shopSuccessTo2>view:nth-child(3){
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 104rpx;
}
.shopSuccessTo2>image:nth-child(4){
  width:100% ;
  height: 650rpx;
}
/* 扫码 */
.rightSm{
  width: 100%;
  margin-top: 92rpx;
  text-align: center;
}
.rightSm>image{
  width: 196rpx;
  height: 196rpx;
  margin-bottom: 34rpx;
}
.rightSm>view{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
}
/* 第二种布局写法 */
.shopSuccessTwo{
  width: 100%;
  height: 100%;
  background: #fff;
}
.shopSuccessTwo>view:nth-child(1){
  text-align: center;
  margin: 66rpx 0 46rpx;
  color: #5D9BE5;
  font-weight: bold;
  font-size: 40rpx;
}
/*  */
.shopSuccessMsg{
  padding: 0 25rpx;
}
.shopSuccessMsg1{
  font-size: 26rpx;
  color: #696969;
  padding: 0 36rpx 0 26rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 638rpx;
  height: 150rpx;
  border-radius: 12rpx 12rpx 0 0;
  box-shadow: 1rpx 2rpx 10rpx 0 rgba(203,203,203,.5),-1rpx -2rpx 10rpx 0 rgba(203,203,203,.5);
}
.shopSuccessMsg2>view:nth-child(1){
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 8rpx;
  color: #000;
}
.shopSuccessMsg3{
  width: 160rpx;
  height: 44rpx;
  background: #F6F6F6;
  border-radius: 4rpx;
  color:  #4A4A4A;
  text-align: center;
  line-height: 44rpx;
}
.shopSuccessMsg3>image{
  width: 12rpx;
  height: 20rpx;
  transform: rotate(180deg);
}
.shopSuccessMsgCard{
  background:#F0ECF9 ;
  box-shadow: 1rpx 2rpx 10rpx 0 rgba(203,203,203,0.5);
border-radius: 0 0 12rpx 12rpx;
  padding: 32rpx 0;
  margin-bottom: 20rpx;
}
.shopSuccessMsgCardOne{
  display: flex;
  height: 64rpx;
  line-height: 64rpx;
  padding-left: 20rpx;
  text-align: center;
  color: #404040;
  font-size: 26rpx;
  margin-bottom: 32rpx;
}
.shopSuccessMsgCardOne>view:nth-child(2){
  width: 428rpx;
  height: 64rpx;
  margin:0 22rpx 0 20rpx;
  border-radius: 12rpx;
  background: #fff;
}
.shopSuccessMsgCardOne>view:nth-child(3){
  width: 134rpx;
  height: 64rpx;
  margin-right: 24rpx;
  border-radius: 12rpx;
  background: #fff;
  border-radius: 12rpx;
  font-size: 22rpx;
  
}
/*  */
.shopSuccesShow{
  text-align: center;
  font-size: 26rpx;
  color: #939393;
  margin: 40rpx 0 26rpx;
}
.shopSuccesShow>text{
  color: #4A90E2;
}
.bg{
  width: 100%;
  height: 1334rpx;
}